transition()
	-webkit-transition arguments
	-moz-transition arguments
	-ms-transition arguments
	-o-transition arguments
	transition arguments

box-sizing()
	-webkit-box-sizing arguments
	-moz-box-sizing arguments
	box-sizing arguments

border-radius()
	-webkit-border-radius arguments
	-moz-border-radius arguments
	-ms-border-radius arguments
	-o-border-radius arguments
	border-radius arguments

box-shadow()
	-webkit-box-shadow arguments
	-moz-box-shadow arguments
	-ms-box-shadow arguments
	-o-box-shadow arguments
	box-shadow arguments

opacity(n)
	opacity n
	if support-for-ie
		filter unquote('progid:DXImageTransform.Microsoft.Alpha(Opacity=' + round(n * 100) + ')')

article#home-menu 
	top 0px
	left 0px
	background-color black
	width 192px
	& nav
		width 100%
		& a 
			text-transform uppercase
			font-size .7em
			text-decoration none
			color #6d6a67
			font-weight bold
		& ul 
			padding 0
		& li
			border .1em solid gray
			display inline-block
			height 65px
			float left
			width 94px
			text-align center
			&:first-line 
				line-height 65px
	& footer
		border-top .1em solid gray
		bottom 0px;
		color #858585
		height 100px
		position absolute
		text-align center
		width 100%

article#home-article-principal
	box-sizing border-box
	top 0px
	left 192px
	background transparent
	border-right 1px dotted white
	width 461px
	padding 20px 40px
	& header h1
		margin-top 1.3em
		line-height .8em
		font-size 3.5em
		text-align right
		text-transform uppercase
		border-bottom 1px solid white
	& header h2
		text-align center
		line-height .6em
		font-size 1.2em
		position relative 
		top -33px
	& div.contenido-article
		margin-top 2em
		text-align center
		line-height 24px
		padding-bottom 2.5em
		background url("../images/keyarts/home/separador-horizontal.png") no-repeat center bottom 


article#home-article-secundario
	box-sizing border-box
	top 0px
	left 653px
	background transparent
	border-right 1px dotted white		
	width 371px
	padding 40px 72px
	position absolute
	& .marginTop60
		margin-top 9em
	& .marginTop30
		margin-top 2em
	& h3
		font-size .9em 
		font-weight bold
		text-transform uppercase
		& img
			position relative
			right -20px
			bottom -15px
	& footer
		position absolute
		bottom 0px

body
	color white
	background-color black
	font-size 16px

section
	background url("../images/keyarts/home/backgroundhome.png") no-repeat 100% 100%
	width 100%
	min-height 100%
	position absolute
	
section > article
	min-height 100% 
	position absolute

#container
	left 0px
	min-height 100%
	top 0px
	position absolute
	width 100%

#galeria-principal
	margin-top 24px
	font-size 1.7em
	width 100%
	position relative
	& #contenido-galeria
		position relative
		height 217px
		width 384px
		text-align center
		overflow hidden
	& #ctl-left
		position absolute
		top 110px
		left 0px
		z-index 1000
	& #ctl-right
		position absolute
		top 110px
		right 0px
		z-index 1000

#logo
	margin 65px 16px 57px
	width 100%
	& img
		width 160px

p.seleccion-menu
	color #ffc157
	text-transform capitalize
	line-height 65px
	text-align center

ul.slider 
	padding 0px
	width 384px
	& > li
		display inline-block
		vertical-align top
		& img
			width 384px