transition()
	-webkit-transition arguments
	-moz-transition arguments
	-ms-transition arguments
	-o-transition arguments
	transition arguments

box-sizing()
	-webkit-box-sizing arguments
	-moz-box-sizing arguments
	box-sizing arguments

border-radius()
	-webkit-border-radius arguments
	-moz-border-radius arguments
	-ms-border-radius arguments
	-o-border-radius arguments
	border-radius arguments

box-shadow()
	-webkit-box-shadow arguments
	-moz-box-shadow arguments
	-ms-box-shadow arguments
	-o-box-shadow arguments
	box-shadow arguments

transform()
	-webkit-transform arguments
	-moz-transform arguments
	-ms-transform arguments
	-o-transform arguments
	transform arguments

opacity(n)
	opacity n
	if support-for-ie
		filter unquote('progid:DXImageTransform.Microsoft.Alpha(Opacity=' + round(n * 100) + ')')

article#home-menu 
	background #1e1e1e
	height 100%
	left 0px
	top 0px
	width 192px
	z-index 1000
	& header
		position relative 
		top 7%
		bottom 7%
	& nav
		position relative
		top 12%
		width 100%
		& ul 
			padding 0
			width 192px
			height 65px
		& li
			float left
			width 96px
			height 65px
			margin 0px
			display inline-block
		& li a
			width 100%
			height 100%
			display block
			text-decoration none
		& li a.link-home
			background url('../images/keyarts/navegador/menusprite_aprobar.png') no-repeat -4px 0px
			/*transition background .5s ease*/
		& li a.link-home:hover
		& li a.link-home.active
			background url('../images/keyarts/navegador/menusprite_aprobar.png') no-repeat -4px -65px
		& li a.link-works
			background url('../images/keyarts/navegador/menusprite_aprobar.png') no-repeat -97px 0px
			/*transition background .5s ease*/
		& li a.link-works:hover
		& li a.link-works.active
			background url('../images/keyarts/navegador/menusprite_aprobar.png') no-repeat -97px -65px
			
	& span.ctl-visualizar-menu
		background url('../images/keyarts/navegador_inside/expandir.png') no-repeat center center;
		display block
		height 146px
		right 0px
		top 50%
		position absolute
		opacity .1
		transition all 1s
		margin-top -50px
		width 54px
		z-index 1000
	& span.ctl-visualizar-menu:hover
		cursor pointer
		opacity 1
	& span.ctl-visualizar-menu.active
		cursor pointer
		transform rotateY(-180deg)
		opacity 1
	& footer
		border-top .1em solid #313131
		bottom 0px;
		font-family 'open_sanslight'
		font-size 15px
		color #858585
		height 100px
		position absolute
		text-align center
		width 100%

