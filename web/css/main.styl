article#proyectos-article
	box-sizing border-box
	top 0px
	left 0px
	width 100%
	height 100%
	padding-left 192px
	overflow hidden
	background-color black
	& span.ctl-galeria
		display block
		height 71px
		left 100%
		margin-top -24px
		position absolute
		top 50%
		width 49px
		z-index 2000
	& span.ctl-galeria-left
		background url(../images/keyarts/works/arrow_left_normal.png) no-repeat center center
		cursor pointer
		margin-left -110px
		transition all 1s ease
		opacity .6
	& span.ctl-galeria-left:hover
		background url(../images/keyarts/works/arrow_left_over.png) no-repeat center center
		opacity 1
	& span.ctl-galeria-right
		background url(../images/keyarts/works/arrow_right_normal.png) no-repeat center center
		cursor pointer
		margin-left -55px
		transition all 1s ease
		opacity .6
	& span.ctl-galeria-right:hover
		background url(../images/keyarts/works/arrow_right_over.png) no-repeat center center
		opacity 1
	& header
		box-sizing border-box
		height 50px
		padding-top 10px
		padding-left 20px
		background #313131
		& span
			cursor pointer
		& #titulos-proyectos
			float left
		& #controles-proyectos
			float left
			margin-left 20px
			& > span 
				height 30px
				width 25px
				content ''
				display inline-block
				vertical-align top
				margin-left 20px			
			& .ctl-left
				background url(../images/keyarts/works/header_arrowleft_normal.png) no-repeat center center
			& .ctl-left:hover
				background url(../images/keyarts/works/header_arrowleft_over.png) no-repeat center center
			& .ctl-right
				background url(../images/keyarts/works/header_arrowright_normal.png) no-repeat center center
			& .ctl-right:hover
				background url(../images/keyarts/works/header_arrowright_over.png) no-repeat center center		
		& #visualizar-proyectos
			float right
			margin-right 20px
			& > span 
				height 30px
				width 25px
				content ''
				display inline-block
				vertical-align top
			& .vcells
				background url(../images/keyarts/works/vista_grid_normal.png) no-repeat center center
			& .vcells:hover
			& .vcells.active
				background url(../images/keyarts/works/vista_grid_over.png) no-repeat center center
			& .vsingle
				background url(../images/keyarts/works/vista_full_normal.png) no-repeat center center
			& .vsingle:hover
			& .vsingle.active
				background url(../images/keyarts/works/vista_full_over.png) no-repeat center center		



